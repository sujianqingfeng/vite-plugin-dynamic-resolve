#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/white-letter/work-space/i/vite-plugin-dynamic-resolve/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/white-letter/work-space/i/vite-plugin-dynamic-resolve/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jiti@1.13.0/node_modules/jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../../../../../jiti@1.13.0/node_modules/jiti/bin/jiti.js" "$@"
fi
